      <Button fx:id="suprimer" layoutX="44.0" layoutY="267.0" mnemonicParsing="false" onAction="#OnClickSuprimer" prefHeight="26.0" prefWidth="103.0" text="Suprimer" />
      <Button fx:id="ajouter" layoutX="44.0" layoutY="301.0" mnemonicParsing="false" onAction="#OnClickAjouter" prefHeight="26.0" prefWidth="103.0" text="Ajouter" />
